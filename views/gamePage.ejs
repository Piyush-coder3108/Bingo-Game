  <div class="timeline">
    <h3>Timeline</h3>
    <p>Heelo</p>
    <p>Heelo</p>
    <p>Heelo</p>
    <p>Heelo</p>
  </div>

  <div class="chatbox">
    <h3>Chatbox</h3>
    <p>Heelo</p>
    <p>Heelo</p>
    <p>Heelo</p>
    <p>Heelo</p>
  </div>



  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
